{
  "name": "isumanbanerjee/framework",
  "description": "A lightweight and modular PHP framework designed for simplicity, performance, and flexibility. Ideal for developers who prefer clean, concise, and efficient code, this framework empowers you to build scalable and secure web applications without the overhead of larger frameworks.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Suman Banerjee",
      "email": "contact@isumanbanerjee.com"
    }
  ],
  "config": {
    "vendor-dir": "resources/vendor"
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "resources/npm",
      "bower-asset-library": "resources/bower"
    }
  },
  "require": {
    "twbs/bootstrap": "v5.3.2",
    "twbs/bootstrap-icons": "v1.10.5",
    "components/jquery": "3.4.1",
    "phpmailer/phpmailer": "v6.8.0",
    "nnnick/chartjs": "v4.3.3",
    "ramsey/uuid": "4.7.4",
    "matthiasmullie/minify": "1.3.70",
    "voku/html-min": "4.5.0",
    "daneden/animate.css": "3.5.3",
    "melbahja/seo": "v2.1.1",
    "ext-curl": "*",
    "ext-fileinfo": "*",
    "ext-openssl": "*",
    "ext-pdo": "*"
  },
  "autoload": {
    "psr-4": {
      "Core\\": "Core/",
      "Core\\Configuration\\": "Core/Configuration",
      "Core\\Model\\": "Core/Model",
      "Core\\View\\": "Core/View",
      "Core\\Controller\\": "Core/Controller",
      "System\\Model\\": "System/Model",
      "System\\View\\": "System/View",
      "System\\Controller\\": "System/Controller"
    }
  }
}